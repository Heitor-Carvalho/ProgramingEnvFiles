# Usefull alias
alias ll='ls -alF'
alias la='ls -A'
alias l='ls -CF'
alias f='find -name'
alias du='du -d 1 -h'
alias df='df -h'
alias bpython='python -m bpython'
alias na='nautilus'

# Parenting changing perms on / #
alias chown='chown --preserve-root'
alias chmod='chmod --preserve-root'
alias chgrp='chgrp --preserve-root'

# Pass options to free ##
alias meminfo='free -m -l -t -h'

# Get server cpu info ##
alias cpuinfo='lscpu'


